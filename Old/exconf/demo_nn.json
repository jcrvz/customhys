{
  "ex_config": {
    "experiment_name": "demo_nn_learning",
    "experiment_type": "neural_network",
    "heuristic_collection_file": "short_collection.txt",
    "use_parallel": true,
    "parallel_pool_size": 1,
    "machine_learning_params": {
      "num_replicas": 10,
      "delete_idx": 5,
      "model_params": {
        "load_model": false,
        "save_model": false,
        "sample_sequences_params": {
          "filters": {
            "features": null,
            "include_dimensions": true,
            "include_population": true
          },
          "retrieve_sequences": false,
          "generate_sequences": true,
          "store_sequences": true,
          "kw_weighting_params": {
            "include_fitness": false,
            "learning_portion": 1
          }
        },
        "encoder" : "Ragged",
        "include_fitness": true,
        "fitness_to_weight": "rank",
        "model_architecture": "LSTM_Ragged",
        "model_architecture_layers": [
          [15, "sigmoid"]
        ],
        "epochs": 300, 
        "include_early_stopping": true,
        "early_stopping_params": {
          "monitor": "accuracy",
          "patience": 20,
          "min_delta": 0.0025,
          "mode": "max"
        }
      }
    }
  },
  "hh_config": {
    "num_steps": 100,
    "num_agents": 30,
    "num_iterations": 100,
    "cardinality": 3,
    "cardinality_min": 1,
    "num_replicas": 100,
    "stagnation_percentage": 0.50,
    "as_mh": false,
    "verbose": true,
    "repeat_operators": true,
    "allow_weight_matrix": true,
    "trial_overflow": false
  },
  "prob_config": {
    "dimensions": [
      2,
      10,
      30,
      50
    ],
    "functions": [
      "<choose_randomly>"
    ]
  }
}